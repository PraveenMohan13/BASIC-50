#include <stdio.h>
int main() {
  int n=5,ans=1;
  for(int i=1;i<=n;i++)
  ans=ans*i;
  printf("%d",ans);
}
-------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
int main() {
  int n=5,ans=1;
  while(n>0)
  {
  ans=ans*n;
  n--;
  }
  printf("%d",ans);
}
-------------------------------------------------------------------------------------------------------------------------------------------------
