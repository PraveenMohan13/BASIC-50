#include <stdio.h>
int main() {
  int n,ans=1,b;
  scanf("%d %d",&n,&b);
  for(int i=0;i<b;i++)
      ans=ans*n;
  printf("%d",ans);
}
------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include<math.h>
int main() {
  int n,b,ans;
  scanf("%d %d",&n,&b);
  ans=pow(n,b);
  printf("%d",ans);
}
------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include<math.h>
int main() {
  int n,b,ans;
  scanf("%d %d",&n,&b);
  printf("%lf",pow(n,b));
}
------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include<math.h>
int main() {
  int n,b,ans;
  scanf("%d %d",&n,&b);
  printf("%g",pow(n,b));
}
------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include<math.h>
int main() {
  int n,b,ans;
  scanf("%d %d",&n,&b);
  printf("%d",(int)pow(n,b));
}
------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
#include<math.h>
int main() {
  int n,b,ans=1;
  scanf("%d %d",&n,&b);
 while(b>0)
 {
     ans=ans*n;
     b--;
 }
 printf("%d",ans);
}
