#include<stdio.h>
int main() {
  int n,i,ans=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    ans=a[0];
    for(int i=1;i<n;i++)
    {
        if(ans>a[i])
            ans=a[i];
    }
    printf("%d",ans);
}
        
        
        
        
        
        
        
            
        
        

