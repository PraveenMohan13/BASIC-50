#include <stdio.h>
int main() 
{
int n,a=0,b=1,c;
scanf("%d",&n);
if(n==0||n==1)
{
    printf("%d",n);
    return 0;
}
for(int i=2;i<=n;i++)
{
    c=a+b;
    a=b;
    b=c;
}
printf("%d",c);
}
----------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
int main() 
{
int n;
scanf("%d",&n);
int a[n+1];
a[0]=0;
a[1]=1;
for(int i=2;i<=n;i++)
{
    a[i]=a[i-1]+a[i-2];
}
printf("%d",a[n]);
}
----------------------------------------------------------------------------------------------------------------------------------------------------
#include <stdio.h>
int fun(int n)
{
    if(n<=1)
    return n;
    return fun(n-1)+fun(n-2);
}
int main() 
{
    int n;
    scanf("%d",&n);
    printf("%d",fun(n));
}
